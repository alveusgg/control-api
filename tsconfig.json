{
	"compilerOptions": {
		"target": "ESNext",
		"moduleResolution": "bundler",
		"strict": true,
		"verbatimModuleSyntax": true,
		"skipLibCheck": true,
		"erasableSyntaxOnly": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"types": ["node"],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"outDir": "./dist",
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"]
		}
	},
	"include": ["src", "test3.ts", "pretty-js-log.d.ts", "vite.config.ts"],
	"exclude": ["node_modules"]
}
